    "vuetify": "^1.5.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chrome": "^0.0.86",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^3.1.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
