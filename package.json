    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^3.0.1",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
